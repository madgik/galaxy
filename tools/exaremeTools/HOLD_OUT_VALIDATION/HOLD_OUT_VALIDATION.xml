<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<tool version="1.0.0" name="HOLD OUT VALIDATION" id="HOLD_OUT_VALIDATION">
    <description>HOLD OUT VALIDATION</description>
        <command interpreter="python">HOLD_OUT_VALIDATION.py -p '$pathology' -d '$dataset' -x '$x' -y '$y' -tes '$test_size' -trs '$train_size' -rans '$random_state' -sh '$shuffle' -o '$output' </command>
    <inputs>
        <param type="text" name="pathology" label="Pathology" help=""/>
        <param type="text" name="dataset" label="Dataset" help=""/>
        <param type="text" name="x" label="X" help="Column"/>
        <param type="text" name="y" label="Y" help="classname"/>
        <param type="text" name="test_size" label="Test Size" help="Default value 0.25" optional="true" />
        <param type="text" name="train_size" label="Train Size" help="" optional="true" />
        <param type="text" name="random_state" label="Random State" help="" optional="true" />
        <param type="text" name="shuffle" label="Shuffle" help="" optional="true" />
    </inputs>
    <outputs>
        <data name="output" format="json" />
    </outputs>
</tool>