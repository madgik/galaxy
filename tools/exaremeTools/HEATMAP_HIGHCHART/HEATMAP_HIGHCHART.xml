<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<tool version="1.0.0" name="HEATMAP HIGHCHART" id="HEATMAP_HIGHCHART">
    <description>HEATMAP HIGHCHART</description>
        <command interpreter="python">HEATMAP_HIGHCHART.py -in '$input' -t '$title' -xt '$xTitle' -yt '$yTitle' -o '$output' </command>
    <inputs>
		<param name="input" format="json" type="data" label="" help="" />
		<param name="title" type="text" label="Title" help="" />
		<param name="xTitle" type="text" label="x Axis Title" help="" />
		<param name="yTitle" type="text" label="y Axis Title" help="" />
    </inputs>
    <outputs>
        <data name="output" format="json" />
    </outputs>
</tool>