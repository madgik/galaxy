<VirtualHost *:80>
  ServerName {{ host_fqdn if host_fqdn is defined else hostvars[groups['galaxy'][0]].ansible_fqdn }}

  ProxyPass / http://localhost:{{executor_port}}/
  ProxyPassReverse / http://localhost:{{executor_port}}/
  ProxyPass /editor http://localhost:{{editor_port}}/editor
  ProxyPassReverse /editor http://localhost:{{editor_port}}/editor

</VirtualHost>
